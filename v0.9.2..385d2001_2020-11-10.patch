+    src/errors.h
+    src/filename_regex.c
+    src/filename_regex.h
+    src/freedb.c
+    src/freedb.h
+    src/freedb_utils.c
+    src/freedb_utils.h
+    src/input_output.c
+    src/input_output.h
+    src/mp3splt.c
+    src/oformat_parser.c
+    src/oformat_parser.h
+    src/options.c
+    src/options.h
+    src/output_format.c
+    src/output_format.h
+    src/pair.c
+    src/pair.h
+    src/plugins.c
+    src/plugins.h
+    src/proxy.c
+    src/proxy.h
+    src/silence_utils.c
+    src/silence_utils.h
+    src/socket_manager.c
+    src/socket_manager.h
+    src/split_points.c
+    src/split_points.h
+    src/splt.c
+    src/splt.h
+    src/splt_array.c
+    src/splt_array.h
+    src/string_utils.c
+    src/string_utils.h
+    src/sync_errors.c
+    src/sync_errors.h
+    src/tags_parser.c
+    src/tags_parser.h
+    src/tags_utils.c
