+include_directories(${LIBINTL_INCLUDE_DIR})
+target_link_libraries(mp3splt ${LIBINTL_LIBRARIES})
+
+find_package(Gettext REQUIRED)
+include_directories(${GETTEXT_INCLUDE_DIR})
